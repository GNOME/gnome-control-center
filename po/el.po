msgid "Strength: Medium"
msgstr "Ισχύς: Μέτρια"

#: ../panels/user-accounts/pw-utils.c:151
#: ../panels/user-accounts/pw-utils.c:174
msgctxt "Password strength"
msgid "Strength: Good"
msgstr "Ισχύς: Καλή"

#: ../panels/user-accounts/pw-utils.c:154
#: ../panels/user-accounts/pw-utils.c:175
msgctxt "Password strength"
msgid "Strength: High"
msgstr "Ισχύς: Υψηλή"

