{
    "app-id" : "org.gnome.Settings",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "master",
    "sdk" : "org.gnome.Sdk",
    "command" : "gnome-control-center",
    "rename-desktop-file" : "gnome-control-center.desktop",
    "tags" : [
        "devel"
    ],
    "desktop-file-name-prefix" : "(Development) ",
    "finish-args" : [
        "--device=dri",
        "--env=DCONF_USER_CONFIG_DIR=.config/dconf",
        "--filesystem=host",
        "--own-name=org.gnome.ControlCenter",
        "--own-name=org.gnome.SessionManager",
        "--share=ipc",
        "--share=network",
        "--socket=x11",
        "--socket=pulseaudio",
        "--socket=session-bus",
        "--socket=system-bus",
        "--socket=wayland"
    ],
    "build-options" : {
        "cflags" : "-O2 -g",
        "cxxflags" : "-O2 -g",
        "env" : {
            "V" : "1"
        }
    },
    "x-run-args" : [
        "--verbose"
    ],
    "cleanup" : [
        "/include",
        "/share/aclocal",
        "/man",
        "/share/man",
        "/share/gtk-doc",
        "/share/vala",
        "*.la",
        "*.a"
    ],
    "modules" : [
