<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="CcDeviceComboRow" parent="AdwComboRow">
    <property name="model">
      <object class="GListStore" id="device_list">
        <property name="item-type">GvcMixerUIDevice</property>
      </object>
    </property>
    <property name="factory">
      <object class="GtkSignalListItemFactory">
        <signal name="setup" handler="factory_setup_cb" object="CcDeviceComboRow"/>
        <signal name="bind" handler="factory_bind_cb" object="CcDeviceComboRow"/>
        <signal name="unbind" handler="factory_unbind_cb" object="CcDeviceComboRow"/>
      </object>
    </property>
  </template>
</interface>
