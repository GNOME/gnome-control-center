<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="CcFormatPreview" parent="AdwPreferencesGroup">

    <style>
      <class name="format-preview"/>
    </style>

     <!-- Dates -->
    <child>
      <object class="AdwActionRow" id="date_format_row">
        <property name="title" translatable="yes">Dates</property>
        <style>
          <class name="property"/>
        </style>
      </object>
    </child>

    <!-- Times -->
    <child>
      <object class="AdwActionRow" id="time_format_row">
        <property name="title" translatable="yes">Times</property>
        <style>
